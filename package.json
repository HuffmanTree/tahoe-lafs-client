{
  "name": "tahoe-lafs-client",
  "version": "1.5.0",
  "description": "Javascript client for the Tahoe-LAFS Web API written in Typescript",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*-test.ts",
    "build": "tsc -p .",
    "typedoc_gen": "typedoc --out doc/",
    "typedoc_build": "npm run typedoc_gen && zip -r doc/tsdoc.zip doc/",
    "typedoc_serve": "npm run typedoc_gen && http-server doc/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/HuffmanTree/tahoe-lafs-client.git"
  },
  "keywords": [
    "Tahoe-LAFS",
    "Javascript",
    "Typescript",
    "API"
  ],
  "author": "François Guérin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HuffmanTree/tahoe-lafs-client/issues"
  },
  "homepage": "https://github.com/HuffmanTree/tahoe-lafs-client#readme",
  "dependencies": {
    "@types/form-data": "^2.5.0",
    "@types/node": "^12.11.2",
    "@types/node-fetch": "^2.5.2",
    "form-data": "^2.5.1",
    "node-fetch": "^2.6.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "js-yaml": ">=3.13.1",
    "lodash": ">=4.17.13",
    "mocha": "^6.1.4",
    "ts-node": "^8.4.1",
    "typedoc": "^0.15.0"
  }
}
